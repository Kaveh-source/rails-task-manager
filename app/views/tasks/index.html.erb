<h1>Tasks</h1>

<% @tasks.each do |t| %>
  <h1><%= link_to t.title, task_path(t) %></h1>
  <p><%= t.details %></p>
  <%= t.completed %>
  <%= link_to 'Edit', edit_task_path(t)  %>
  <%= link_to 'Delete', destroy_task_path(t), method: :delete  %>
<% end %>
<p><%= link_to 'Create a task', create_path %></p>



